 /*                                                                 */  00162000
 /*******************************************************************/  00163000
 /* THIS JOB ALLOCATES AN INTEGRATED CATALOG FACILITY (ICF) CATALOG */  00164000
 /* AND ITS ASSOCIATED ALIAS "?UID".                                */  00165000
 /*                                                                 */  00166000
 /*  ****** INTEGRATED CATALOG FACILITY CATALOG REQUIRED *******    */  00167000
 /*                                                                 */  00168000
 /* THIS JOB ALLOCATES A USER CATALOG FOR THE DFSMSHSM CONTROL DATA */  00169000
 /* SETS (CDS). SEE THE SECTION "DFSMSHSM DATA SETS" IN THE         */  00170000
 /* DFSMSHSM IMPLEMENTATION AND CUSTOMIZATION GUIDE.                */  00171000
 /*******************************************************************/  00172000
 /*                                                                 */  00173000
  DEFINE UCAT(NAME(?UCATNAM) -                                          00174000
         CYLINDERS(1 1) VOLUME(?UCATVOL) -                              00175000
         FILE(HSMCAT) FREESPACE(10 10) -                                00176000
         RECORDSIZE(4086 4086) -                                        00177000
         ICFCATALOG)                                                    00178000
  IF MAXCC = 0 THEN DO                                                  00179000
    DEFINE  ALIAS(NAME(?UID) RELATE(?UCATNAM))                          00180000
  END                                                                   00181000
 /*                                                              */     00182000
 /****************************************************************/     00183000
 /*  THIS PROCEDURE ASSUMES A SINGLE CLUSTER MCDS.  IF MORE THAN */     00184000
 /*  ONE VOLUME IS DESIRED, FOLLOW THE RULES FOR A MULTICLUSTER  */     00185000
 /*  CDS.                                                        */     00186000
 /****************************************************************/     00187000
 /*                                                              */     00188000
 IF MAXCC = 0 THEN DO                                                   00189000
   DEFINE CLUSTER (NAME(?UID.MCDS) VOLUMES(?MCDSVOL) -                  00190000
        CYLINDERS(?CDSSIZE) FILE(HSMMCDS) -                             00191000
        STORCLAS(?SCCDSNM) -                                            00192000
        MGMTCLAS(?MCDFHSM) -                                            00193000
        RECORDSIZE(435 2040) FREESPACE(0 0) -                           00194000
        INDEXED KEYS(44 0) SHAREOPTIONS(3 3) -                          00195000
        SPEED BUFFERSPACE(530432) -                                     00196000
        UNIQUE NOWRITECHECK) -                                          00197000
        DATA(NAME(?UID.MCDS.DATA) -                                     00198000
        CONTROLINTERVALSIZE(12288)) -                                   00199000
        INDEX(NAME(?UID.MCDS.INDEX) -                                   00200000
        CONTROLINTERVALSIZE(2048))                                      00201000
 END                                                                    00202000
 /*                                                              */     00203000
 /****************************************************************/     00204000
 /* REMOVE THE NEXT DEFINE COMMAND IF YOU DO NOT                 */     00205000
 /* INTEND TO USE BACKUP, DUMP OR AGGREGATE BACKUP AND RECOVERY. */     00206000
 /*                                                              */     00207000
 /* THIS PROCEDURE ASSUMES A SINGLE CLUSTER BCDS.  IF MORE THAN  */     00208000
 /* ONE VOLUME IS DESIRED, FOLLOW THE RULES FOR A MULTICLUSTER   */     00209000
 /* CDS.                                                         */     00210000
 /*                                                              */     00211000
 /* IT'S RECOMMENDED THAT YOU SHOULD SPECIFY RECORDSIZE(334 2093)*/     00212000
 /* AND CISIZE(12288) WHEN CREATING UP TO 29 BACKUP VERSIONS     */     00213000
 /* OR RECORDSIZE(334 6544) AND CISIZE(12288) IF UP TO           */     00214000
 /* 100 BACKUP VERSIONS WILL BE KEPT OR IF FAST REPLICATION IS   */     00215000
 /* BEING USED (FRBACKUP).                                       */     00216000
 /*                                                              */     00217000
 /****************************************************************/     00218000
 /*                                                              */     00219000
 IF MAXCC = 0 THEN DO                                                   00220000
   DEFINE CLUSTER (NAME(?UID.BCDS) VOLUMES(?BCDSVOL) -                  00221000
        CYLINDERS(?CDSSIZE) FILE(HSMBCDS) -                             00222000
        STORCLAS(?SCCDSNM) -                                            00223000
        MGMTCLAS(?MCDFHSM) -                                            00224000
        RECORDSIZE(334 6544) FREESPACE(0 0) -                           00225000
        INDEXED KEYS(44 0) SHAREOPTIONS(3 3) -                          00226000
        SPEED BUFFERSPACE(530432) -                                     00227000
        UNIQUE NOWRITECHECK) -                                          00228000
        DATA(NAME(?UID.BCDS.DATA) -                                     00229000
        CONTROLINTERVALSIZE(12288)) -                                   00230000
        INDEX(NAME(?UID.BCDS.INDEX) -                                   00231000
        CONTROLINTERVALSIZE(2048))                                      00232000
 END                                                                    00233000
 /*                                                                     00234000
 /****************************************************************/     00235000
 /* REMOVE THE NEXT DEFINE COMMAND IF YOU DO NOT                 */     00236000
 /* INTEND TO USE TAPES FOR DAILY BACKUP, SPILL BACKUP, OR       */     00237000
 /* MIGRATION LEVEL 2 PROCESSING.                                */     00238000
 /*                                                              */     00239000
 /*                                                              */     00240000
 /* IT IS RECOMMENDED THAT YOU SPECIFY                           */     00241000
 /*   RECORDSIZE(1800 2040) WHEN NOT USING EXTENDED TTOCS AND    */     00242000
 /*   RECORDSIZE(1080 6144) WHEN USING EXTENDED TTOCS.           */     00243000
 /*                                                              */     00244000
 /* NOTE: YOU CAN ONLY USE EXTENDED TTOCS IF ALL OF YOUR         */     00245000
 /* DFSMSHSM HOSTS ARE AT Z/OS DFSMSHSM V1R7 OR LATER.           */     00246000
 /*                                                              */     00247000
 /* THE OCDS MAY NOT EXCEED 1 VOLUME.                            */     00248000
 /****************************************************************/     00249000
 /*                                                                     00250000
 IF MAXCC = 0 THEN DO                                                   00251000
   DEFINE CLUSTER (NAME(?UID.OCDS) VOLUMES(?OCDSVOL) -                  00252000
        CYLINDERS(?CDSSIZE) FILE(HSMOCDS) -                             00253000
        STORCLAS(?SCCDSNM) -                                            00254000
        MGMTCLAS(?MCDFHSM) -                                            00255000
