//ARCPRINT DD SYSOUT=*                                                  02036000
//ARCLOG DD DSN=?UID.EDITLOG,DISP=SHR                                   02037000
/*                                                                      02038000
./ ADD NAME=ALLOCBK1                                                    02039000
//ALLOCBK1 JOB ?JOBPARM                                                 02040000
//ALLOCBK EXEC PGM=IEFBR14                                              02041000
//*                                                                     02042000
//*****************************************************************/    02043000
//*    THIS SAMPLE JOB ALLOCATES AND CATALOGS THE CONTROL DATA SET*/    02044000
//*    BACKUP VERSION DATA SETS ON DASD VOLUMES.                  */    02045000
//*                                                               */    02046000
//*    ENSURE THAT BACKUP VERSION DATA SETS ARE PLACED ON VOLUMES */    02047000
//*    THAT ARE DIFFERENT FROM THE VOLUMES THAT THE CONTROL DATA  */    02048000
//*    SETS ARE ON.                                               */    02049000
//*                                                               */    02050000
//*    THIS SAMPLE JOB ALLOCATES FOUR BACKUP COPIES (THE DEFAULT) */    02051000
//*    FOR EACH CONTROL DATA SET.  IF YOU SPECIFY A DIFFERENT     */    02052000
//*    NUMBER OF BACKUP VERSIONS, ENSURE THAT YOU ALLOCATE A      */    02053000
//*    BACKUP COPY FOR EACH OF THE BACKUP VERSIONS YOU SPECIFY.   */    02054000
//*****************************************************************/    02055000
//*                                                               */    02056000
//*    EDIT THIS JCL TO REPLACE THE PARAMETERS DESCRIBED BELOW.   */    02057000
//*                                                               */    02058000
//*****************************************************************/    02059000
//*  PARAMETER PARAMETER DEFINITION                                     02060000
//*                                                                     02061000
//*  ?BKUNIT1   -  UNIT TYPE OF VOLUME TO CONTAIN THE FIRST CDS         02062000
//*                BACKUP VERSION.                                      02063000
//*  ?BKUNIT2   -  UNIT TYPE OF VOLUME TO CONTAIN THE SECOND CDS        02064000
//*                BACKUP VERSION.                                      02065000
//*  ?BKUNIT3   -  UNIT TYPE OF VOLUME TO CONTAIN THE THIRD CDS         02066000
//*                BACKUP VERSION.                                      02067000
//*  ?BKUNIT4   -  UNIT TYPE OF VOLUME TO CONTAIN THE FOURTH CDS        02068000
//*                BACKUP VERSION.                                      02069000
//*  ?BKVOL1    -  VOLUME SERIAL OF VOLUME TO CONTAIN THE FIRST CDS     02070000
//*                BACKUP VERSION.                                      02071000
//*  ?BKVOL2    -  VOLUME SERIAL OF VOLUME TO CONTAIN THE SECOND CDS    02072000
//*                BACKUP VERSION.                                      02073000
//*  ?BKVOL3    -  VOLUME SERIAL OF VOLUME TO CONTAIN THE THIRD CDS     02074000
//*                BACKUP VERSION.                                      02075000
//*  ?BKVOL4    -  VOLUME SERIAL OF VOLUME TO CONTAIN THE FOURTH CDS    02076000
//*                BACKUP VERSION.                                      02077000
//*  ?SCBVOL1   -  STORAGE CLASS NAME FOR BACKUP VERSIONS               02078000
//*  ?MCDFHSM   -  MANAGEMENT CLASS NAME OF THE HSM CONSTRUCT           02079000
//*                                                                     02080000
//*  ?CDSSIZE   -  NUMBER OF CYLINDERS ALLOCATED TO CDS BACKUP          02081000
//*                VERSIONS.                                            02082000
//*  ?JNLSIZE   -  NUMBER OF CYLINDERS ALLOCATED TO JOURNAL DATA        02083000
//*                SETS.                                                02084000
//*  ?UID       -  AUTHORIZED USER ID (1 TO 7 CHARS) FOR THE HSM-       02085000
//*                STARTED PROCEDURE. THIS WILL BE USED AS THE          02086000
//*                HIGH-LEVEL QUALIFIER OF HSM DATA SETS.               02087000
//* (NOTE: UID AUTHORIZATION IS VALID IN A NON-FACILITY CLASS    */     02088000
//* ENVIRONMENT ONLY, OTHERWISE, FACILITY CLASS PROFILES WILL BE */     02089000
//* USED FOR AUTHORIZATION CHECKING.)                            */     02090000
//*****************************************************************/    02091000
//*                                                                     02092000
//******************************************************************/   02093000
//* THIS PROCEDURE ASSUMES A SINGLE CLUSTER MCDS.  IF MORE THAN    */   02094000
//* ONE VOLUME IS DESIRED, FOLLOW THE RULES FOR A MULTICLUSTER     */   02095000
//* CDS.                                                           */   02096000
//******************************************************************/   02097000
//*                                                                     02098000
//MCDSV1 DD DSN=?UID.MCDS.BACKUP.V0000001,DISP=(,CATLG),UNIT=?BKUNIT1,  02099000
//   VOL=SER=?BKVOL1,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02100000
//   MGMTCLAS=?MCDFHSM                                                  02101000
//MCDSV2 DD DSN=?UID.MCDS.BACKUP.V0000002,DISP=(,CATLG),UNIT=?BKUNIT2,  02102000
//   VOL=SER=?BKVOL2,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02103000
//   MGMTCLAS=?MCDFHSM                                                  02104000
//MCDSV3 DD DSN=?UID.MCDS.BACKUP.V0000003,DISP=(,CATLG),UNIT=?BKUNIT3,  02105000
//   VOL=SER=?BKVOL3,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02106000
//   MGMTCLAS=?MCDFHSM                                                  02107000
//MCDSV4 DD DSN=?UID.MCDS.BACKUP.V0000004,DISP=(,CATLG),UNIT=?BKUNIT4,  02108000
//   VOL=SER=?BKVOL4,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02109000
//   MGMTCLAS=?MCDFHSM                                                  02110000
//*                                                                     02111000
//******************************************************************/   02112000
//* REMOVE THE NEXT FOUR DD STATEMENTS IF YOU DO NOT INTEND TO USE */   02113000
//* BACKUP AND DUMP                                                */   02114000
//*                                                                */   02115000
//* THIS PROCEDURE ASSUMES A SINGLE CLUSTER BCDS.  IF MORE THAN    */   02116000
//* ONE VOLUME IS DESIRED, FOLLOW THE RULES FOR A MULTICLUSTER     */   02117000
//* CDS.                                                           */   02118000
//******************************************************************/   02119000
//*                                                                     02120000
//BCDSV1 DD DSN=?UID.BCDS.BACKUP.V0000001,DISP=(,CATLG),UNIT=?BKUNIT1,  02121000
//   VOL=SER=?BKVOL1,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02122000
//   MGMTCLAS=?MCDFHSM                                                  02123000
//BCDSV2 DD DSN=?UID.BCDS.BACKUP.V0000002,DISP=(,CATLG),UNIT=?BKUNIT2,  02124000
//   VOL=SER=?BKVOL2,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02125000
//   MGMTCLAS=?MCDFHSM                                                  02126000
//BCDSV3 DD DSN=?UID.BCDS.BACKUP.V0000003,DISP=(,CATLG),UNIT=?BKUNIT3,  02127000
//   VOL=SER=?BKVOL3,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02128000
//   MGMTCLAS=?MCDFHSM                                                  02129000
//BCDSV4 DD DSN=?UID.BCDS.BACKUP.V0000004,DISP=(,CATLG),UNIT=?BKUNIT4,  02130000
//   VOL=SER=?BKVOL4,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02131000
//   MGMTCLAS=?MCDFHSM                                                  02132000
//*                                                                     02133000
//******************************************************************/   02134000
//* REMOVE THE NEXT FOUR DD STATEMENTS IF YOU DO NOT INTEND TO USE */   02135000
//* TAPE VOLUMES FOR DAILY BACKUP VOLUMES, SPILL BACKUP VOLUMES,   */   02136000
//* OR MIGRATION LEVEL 2 VOLUMES.                                  */   02137000
//*                                                                */   02138000
//* THE OCDS MAY NOT EXCEED 1 VOLUME.                              */   02139000
//******************************************************************/   02140000
//*                                                                     02141000
//OCDSV1 DD DSN=?UID.OCDS.BACKUP.V0000001,DISP=(,CATLG),UNIT=?BKUNIT1,  02142000
//   VOL=SER=?BKVOL1,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02143000
//   MGMTCLAS=?MCDFHSM                                                  02144000
//OCDSV2 DD DSN=?UID.OCDS.BACKUP.V0000002,DISP=(,CATLG),UNIT=?BKUNIT2,  02145000
//   VOL=SER=?BKVOL2,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02146000
//   MGMTCLAS=?MCDFHSM                                                  02147000
//OCDSV3 DD DSN=?UID.OCDS.BACKUP.V0000003,DISP=(,CATLG),UNIT=?BKUNIT3,  02148000
//   VOL=SER=?BKVOL3,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02149000
//   MGMTCLAS=?MCDFHSM                                                  02150000
//OCDSV4 DD DSN=?UID.OCDS.BACKUP.V0000004,DISP=(,CATLG),UNIT=?BKUNIT4,  02151000
//   VOL=SER=?BKVOL4,SPACE=(CYL,(?CDSSIZE,5)),STORCLAS=?SCBVOL1,        02152000
//   MGMTCLAS=?MCDFHSM                                                  02153000
//*                                                                     02154000
//JRNLV1 DD DSN=?UID.JRNL.BACKUP.V0000001,DISP=(,CATLG),UNIT=?BKUNIT1,  02155000
//   VOL=SER=?BKVOL1,SPACE=(CYL,(?JNLSIZE,5)),STORCLAS=?SCBVOL1,        02156000
//   MGMTCLAS=?MCDFHSM                                                  02157000
//JRNLV2 DD DSN=?UID.JRNL.BACKUP.V0000002,DISP=(,CATLG),UNIT=?BKUNIT2,  02158000
//   VOL=SER=?BKVOL2,SPACE=(CYL,(?JNLSIZE,5)),STORCLAS=?SCBVOL1,        02159000
//   MGMTCLAS=?MCDFHSM                                                  02160000
//JRNLV3 DD DSN=?UID.JRNL.BACKUP.V0000003,DISP=(,CATLG),UNIT=?BKUNIT3,  02161000
//   VOL=SER=?BKVOL3,SPACE=(CYL,(?JNLSIZE,5)),STORCLAS=?SCBVOL1,        02162000
//   MGMTCLAS=?MCDFHSM                                                  02163000
//JRNLV4 DD DSN=?UID.JRNL.BACKUP.V0000004,DISP=(,CATLG),UNIT=?BKUNIT4,  02164000
//   VOL=SER=?BKVOL4,SPACE=(CYL,(?JNLSIZE,5)),STORCLAS=?SCBVOL1,        02165000
//   MGMTCLAS=?MCDFHSM                                                  02166000
./ ADD NAME=ALLOSDSP                                                    02167000
//ALLOSDSP JOB ?JOBPARM                                                 02168000
//*                                                                     02169000
//*****************************************************************/    02170000
//*   THIS SAMPLE JOB DEFINES AND INITIALIZES A SMALL-DATA-SET-   */    02171000
//*   PACKING DATA SET ON A MIGRATION LEVEL 1 VOLUME.             */    02172000
//*                                                               */    02173000
//*   THE DATA SET NAME IS REQUIRED TO BE ?UID.SMALLDS.V?SDSPVOL  */    02174000
//*   WHERE ?UID IS THE AUTHORIZED DFSMSHSM USER ID AND WHERE     */    02175000
//*   ?SDSPVOL IS THE VOLUME SERIAL NUMBER OF A MIGRATION LEVEL 1 */    02176000
//*   VOLUME.                                                     */    02177000
//*                                                               */    02178000
//* (NOTE: UID AUTHORIZATION IS VALID IN A NON-FACILITY CLASS    *//    02179000
//* ENVIRONMENT ONLY, OTHERWISE, FACILITY CLASS PROFILES WILL BE *//    02180000
//* USED FOR AUTHORIZATION CHECKING.)                            *//    02181000
//*                                                               */    02182000
//*                                                               */    02183000
//*   AFTER YOU ALLOCATE THE SMALL-DATA-SET-PACKING DATA SET ON A */    02184000
//*   MIGRATION LEVEL 1 VOLUME, YOU MUST SPECIFY THE              */    02185000
//*   SMALLDATASETPACKING PARAMETER ON THE SETSYS COMMAND IN THE  */    02186000
//*   ARCCMD__ PARMLIB MEMBER AND ON THE ADDVOL COMMAND FOR THE   */    02187000
//*   VOLUME THAT CONTAINS THE SMALL-DATA-SET-PACKING DATA SET.   */    02188000
//*                                                               */    02189000
//*   CHANGE THE PARAMETERS LISTED BELOW TO VALID VALUES FOR YOUR */    02190000
//*   SYSTEM.                                                     */    02191000
//*****************************************************************/    02192000
//*  PARAMETER  -  PARAMETER DESCRIPTION                                02193000
//*                                                                     02194000
//*  ?SDSPCIS   -  CONTROL INTERVAL SIZE VALUE FOR THE DATA COMPONENT   02195000
//*                OF THE SDSP DATA SET. IF THE SDSP UNIT TYPE IS       02196000
//*                3350, REPLACE ?SDSPCIS WITH 16384.  IF THE SDSP      02197000
//*                UNIT TYPE IS 3380, REPLACE WITH 20480.               02198000
//*                IF THE SDSP UNIT TYPE IS 3390, REPLACE WITH 26624.   02199000
//*  ?SDSPUNT   -  UNIT TYPE FOR MIGRATION LEVEL 1 VOLUME TO            02200000
//*                CONTAIN SMALL-DATA-SET-PACKING DATA SET.             02201000
//*  ?SDSPVOL   -  VOLUME SERIAL OF THE MIGRATION LEVEL 1 VOLUME        02202000
//*                TO CONTAIN SMALL-DATA-SET-PACKING DATA SET.          02203000
//*  ?UID       -  AUTHORIZED USER ID (1 TO 7 CHARS) FOR THE DFSMSHSM   02204000
//*                START PROCEDURE, IN A NON FACILITY CLASS ENVIRONMENT.02205000
//*                USED AS THE HIGH-LEVEL QUALIFIER OF DFSMSHSM         02206000
//*                DATA SETS.                                           02207000
//*                                                                     02208000
//* NOTE:         ENSURE THAT THE SMALL-DATA-SET-PACKING DATA SET       02209000
//*               IS NOT ALLOCATED ON AN SMS VOLUME. THE DATA SET       02210000
//*               SHOULD BE DEFINED IN A STORAGE CLASS FILTER TO        02211000
//*               EXCLUDE IT FROM AN SMS VOLUME AS THE OTHER DFSMSHSM   02212000
//*               DATA SETS ARE.                                        02213000
//*****************************************************************/    02214000
//*****************************************************************/    02215000
//* CREATE A ONE-RECORD, SEQUENTIAL DATA SET TO BE USED LATER TO  */    02216000
//* PRIME VSAM CLUSTERS DEFINED FOR DFSMSHSM.                     */    02217000
//*****************************************************************/    02218000
//*                                                                     02219000
//IEBDG    EXEC PGM=IEBDG                                               02220000
//SYSPRINT DD SYSOUT=*                                                  02221000
//PRIMER   DD DSN=?UID.SDSP.PRIMER,DISP=(NEW,CATLG),UNIT=SYSDA,         02222000
// DCB=(RECFM=F,LRECL=200,BLKSIZE=200),SPACE=(TRK,(1))                  02223000
//SYSIN    DD *                                                         02224000
 DSD OUTPUT=(PRIMER)                                                    02225000
 FD NAME=A,LENGTH=44,STARTLOC=1,PICTURE=44,                            X02226000
   'Z9999999999999999999999999999999999999999999'                       02227000
 CREATE NAME=A                                                          02228000
 END                                                                    02229000
//STEP1 EXEC PGM=IDCAMS,REGION=512K                                     02230000
//SYSPRINT DD SYSOUT=*                                                  02231000
//INPRIMER DD DSN=?UID.SDSP.PRIMER,DISP=(OLD,DELETE)                    02232000
//SDSP1 DD UNIT=?SDSPUNT,VOL=SER=?SDSPVOL,DISP=SHR                      02233000
//SYSIN DD *                                                            02234000
 DEFINE CLUSTER (NAME(?UID.SMALLDS.V?SDSPVOL) VOLUMES(?SDSPVOL) -       02235000
        CYLINDERS(5 0) FILE(SDSP1) -                                    02236000
        RECORDSIZE(2093 2093) FREESPACE(0 0) -                          02237000
        INDEXED KEYS(45 0) -                                            02238000
        SPEED BUFFERSPACE(530432) -                                     02239000
        UNIQUE NOWRITECHECK) -                                          02240000
        DATA -                                                          02241000
        (CONTROLINTERVALSIZE(?SDSPCIS)) -                               02242000
        INDEX -                                                         02243000
        (CONTROLINTERVALSIZE(1024))                                     02244000
 REPRO ODS('?UID.SMALLDS.V?SDSPVOL') INFILE(INPRIMER)                   02245000
./ ADD NAME=HSMPRESS                                                    02246000
//COMPRESS JOB ?JOBPARM                                                 02247000
//*                                                                     02248000
//****************************************************************/     02249000
//*  THIS SAMPLE JOB IS TO COMPRESS THE CONTROL DATA SETS.       */     02250000
//*                                                              */     02251000
//*  NOTE:  BEFORE RUNNING THIS JOB, YOU MUST SHUT DOWN ALL      */     02252000
//*         INSTANCES OF DFSMSHSM THAT SHARE THE CDS'S.          */     02253000
//*                                                              */     02254000
//* REPLACE THE ?UID VARIABLE WITH THE NAME OF THE DFSMSHSM-     */     02255000
//* AUTHORIZED USER ID (1 TO 7 CHARACTERS).                      */     02256000
//*                                                              */     02257000
//* (NOTE: UID AUTHORIZATION IS VALID IN A NON-FACILITY CLASS    */     02258000
//* ENVIRONMENT ONLY, OTHERWISE, FACILITY CLASS PROFILES WILL BE */     02259000
//* USED FOR AUTHORIZATION CHECKING.)                            */     02260000
//*                                                              */     02261000
//* REPLACE THE ?NEW VARIABLE WITH AN EXTENSION NAME FOR THE     */     02262000
//* NEW CDS BEING CREATED FROM THE IMPORT.  BECAUSE OF THE NAME  */     02263000
//* CHANGE MADE TO THE CDS, MAKE SURE TO UPDATE THE ASSOCIATED   */     02264000
//* PROCLIB MEMBER WITH THE NEW CDS NAME(S).                     */     02265000
//*                                                              */     02266000
//* IF YOU WISH TO ENLARGE THE CDS, PREALLOCATE A LARGER DATA    */     02267000
//* SET WITH THE NEW SIZE (EITHER A NEW DATA SET, OR DELETE THE  */     02268000
//* OLD DATA SET AND REALLOCATE WITH SAME NAME) THEN IMPORT.     */     02269000
//* AGAIN, IF A NEW NAME IS USED, BE SURE TO UPDATE THE          */     02270000
//* ASSOCIATED PROCLIB MEMBER WITH THE NEW CDS NAME(S).          */     02271000
//*                                                              */     02272000
//* THIS PROCEDURE ASSUMES THAT THE MCDS AND BCDS ARE SINGLE     */     02273000
//* CLUSTER CDS'S.                                               */     02274000
//*                                                              */     02275000
//* NOTE: XCDS'S DD STMTS WITH DISP=OLD WILL KEEP OTHER JOBS     */     02276000
//*  FROM ACCESSING THE CDS'S DURING THE EXPORT/IMPORT PROCESS.  */     02277000
//****************************************************************/     02278000
//*                                                                     02279000
//ALLOCATE EXEC PGM=IEFBR14                                             02280000
//EXPMCDS DD DSN=?UID.EXPORT.MCDS,DISP=(,CATLG),                        02281000
// UNIT=SYSDA,SPACE=(CYL,(20,2))                                        02282000
//EXPBCDS DD DSN=?UID.EXPORT.BCDS,DISP=(,CATLG),                        02283000
// UNIT=SYSDA,SPACE=(CYL,(20,2))                                        02284000
//EXPOCDS DD DSN=?UID.EXPORT.OCDS,DISP=(,CATLG),                        02285000
// UNIT=SYSDA,SPACE=(CYL,(20,2))                                        02286000
//*                                                                     02287000
//IDCAMS EXEC PGM=IDCAMS,REGION=512K                                    02288000
//MCDS DD DSN=?UID.MCDS,DISP=OLD                                        02289000
//BCDS DD DSN=?UID.BCDS,DISP=OLD                                        02290000
//OCDS DD DSN=?UID.OCDS,DISP=OLD                                        02291000
//SYSPRINT DD SYSOUT=*                                                  02292000
//SYSIN DD *                                                            02293000
 LISTCAT ENT(?UID.MCDS ?UID.BCDS ?UID.OCDS) ALL                         02294000
 EXAMINE NAME(?UID.MCDS) INDEXTEST                                      02295000
 IF LASTCC = 0 THEN -                                                   02296000
        EXPORT ?UID.MCDS ODS(?UID.EXPORT.MCDS) TEMPORARY                02297000
 IF LASTCC = 0 THEN -                                                   02298000
        IMPORT IDS(?UID.EXPORT.MCDS) ODS(?UID.MCDS.?NEW) -              02299000
        OBJECTS -                                                       02300000
         ((?UID.MCDS -                                                  02301000
         NEWNAME(?UID.MCDS.?NEW)) -                                     02302000
          (?UID.MCDS.DATA -                                             02303000
           NEWNAME(?UID.MCDS.?NEW.DATA)) -                              02304000
          (?UID.MCDS.INDEX -                                            02305000
           NEWNAME(?UID.MCDS.?NEW.INDEX))) -                            02306000
        CATALOG(?UCATNAM)                                               02307000
 IF MAXCC = 0 THEN -                                                    02308000
        DELETE ?UID.EXPORT.MCDS NONVSAM                                 02309000
 EXAMINE NAME(?UID.BCDS) INDEXTEST                                      02310000
 IF LASTCC = 0 THEN -                                                   02311000
        EXPORT ?UID.BCDS ODS(?UID.EXPORT.BCDS) TEMPORARY                02312000
 IF LASTCC = 0 THEN -                                                   02313000
        IMPORT IDS(?UID.EXPORT.BCDS) ODS(?UID.BCDS.?NEW) -              02314000
        OBJECTS -                                                       02315000
         ((?UID.BCDS -                                                  02316000
         NEWNAME(?UID.BCDS.?NEW)) -                                     02317000
          (?UID.BCDS.DATA -                                             02318000
           NEWNAME(?UID.BCDS.?NEW.DATA)) -                              02319000
          (?UID.BCDS.INDEX -                                            02320000
           NEWNAME(?UID.BCDS.?NEW.INDEX))) -                            02321000
        CATALOG(?UCATNAM)                                               02322000
 IF MAXCC = 0 THEN -                                                    02323000
        DELETE ?UID.EXPORT.BCDS NONVSAM                                 02324000
 EXAMINE NAME(?UID.OCDS) INDEXTEST                                      02325000
 IF LASTCC = 0 THEN -                                                   02326000
        EXPORT ?UID.OCDS ODS(?UID.EXPORT.OCDS) TEMPORARY                02327000
 IF LASTCC = 0 THEN -                                                   02328000
        IMPORT IDS(?UID.EXPORT.OCDS) ODS(?UID.OCDS.?NEW) -              02329000
        OBJECTS -                                                       02330000
         ((?UID.OCDS -                                                  02331000
         NEWNAME(?UID.OCDS.?NEW)) -                                     02332000
          (?UID.OCDS.DATA -                                             02333000
           NEWNAME(?UID.OCDS.?NEW.DATA)) -                              02334000
          (?UID.OCDS.INDEX -                                            02335000
           NEWNAME(?UID.OCDS.?NEW.INDEX))) -                            02336000
        CATALOG(?UCATNAM)                                               02337000
 IF MAXCC = 0 THEN -                                                    02338000
        DELETE ?UID.EXPORT.OCDS NONVSAM                                 02339000
 LISTCAT ENT(?UID.MCDS.NEW ?UID.BCDS.NEW ?UID.OCDS.NEW) ALL             02340000
/*                                                                      02341000
$$                                                                      02342000
