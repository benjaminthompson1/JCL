//STEP1  EXEC  PGM=ADRDSSU,REGION=0M                                            
//SYSPRINT  DD  SYSOUT=*                                                        
//INVOL1   DD    VOL=SER=USER0A,UNIT=3390,DISP=SHR                              
//SYSIN    DD    *                                                              
  CONVERTV -                                                                    
  DDNAME(INVOL1) -                                                              
  SMS                                                                           
